{
  "name": "b2-cloudflare-worker",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karldreher/b2-cloudflare-worker.git"
  },
  "version": "0.0.0",
  "private": true,
  "release": {
    "branches": [
      "main"
    ]
  },
  "scripts": {
    "deploy": "wrangler deploy",
    "start": "wrangler dev",
    "predev": "echo AUTH_HEADER=$AUTH_HEADER>.dev.vars",
    "dev": "npm run start",
    "test": "concurrently --timings --success first --kill-others \"npm run dev\" \"wait-on -t 30000 http-get://127.0.0.1:8787 && mocha\""
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230717.0",
    "chai": "^4.3.7",
    "concurrently": "^8.2.0",
    "itty-router": "^4.0.14",
    "mocha": "^10.2.0",
    "supertest": "^6.3.3",
    "toml": "^3.0.0",
    "typescript": "^5.1.6",
    "wait-on": "^7.0.1",
    "wrangler": "^3.3.0"
  }
}
